= render 'partial/photo', p: @photo
.text-center.edit-links
  - if @photo.user.id == current_user.id
    = link_to "Edit Post", edit_photo_path(@photo), class:'btn btn-primary'
    = link_to "Cancel", photos_path, class:'btn btn-primary'
  - else
    = link_to "Cancel", photos_path, class:'btn btn-primary'
